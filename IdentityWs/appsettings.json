{
  "Logging": {
    "IncludeScopes": false,
    "LogLevel": {
      "Default": "Warning"
    }
  },
  "SecsBetweenEmailQueueRuns": 120,
  "LockoutPeriodMins": 15,
  "MaxFailedLoginsBeforeLockout": 5,
  "BackgroundJobs": {
    "EmailQueueProcessor": {
      "MinsBetweenInvocations": 2
    },
    "TableCleaner": {
      "MinsBetweenInvocations": 720,
      "DeleteCreatedBeforeDays": {
        "LoginAttempt": 60,
        "Email": 180
      }
    }
  }
}
